<div class="page-header">
  <h1 class= "section-heading" style="color:orange;font-size:500%;font-family:'Contrail One',cursive;">Sign Up</h1>

</div>

<div class="login">
<%= form_tag sessions_path do %>
<div class= "form-group">
  <%= label_tag :Email %>
  <%= email_field_tag :email, params[:email],
  class: "form-control" %>
</div>

<div class="form-group" >
  <% label_tag :Password %>
  <h1>Password</h1>
  <%= password_field_tag :password, nil,
  class: "form-control" %>
</div>

<%= submit_tag "Log In", class: "btn btn-primary" %>
<% end %>
</div>
