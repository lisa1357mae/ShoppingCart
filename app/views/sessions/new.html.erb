<div class="page-header">
  <h1>Log In </h1>
</div>


<div class="login">
<%= form_tag sessions_path do %>
<div class= "form-group">
  <%= label_tag :Email %>
  <%= email_field_tag :email, params[:email],
  class: "form-control" %>
</div>

<div class="form-group" >
  <% label_tag :Password %>
  <h3>Password</h3>
  <%= password_field_tag :password, nil,
  class: "form-control" %>
</div>

<%= submit_tag "Log In", class: "btn btn-primary" %>
<% end %>
</div>
